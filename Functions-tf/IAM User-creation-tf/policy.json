{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "ec2:*",
        "Resource": "*"
      },
      {
        "Effect": "Allow",
        "Action": "vpc:*",
        "Resource": "*"
      },
      {
        "Effect": "Deny",
        "Action": [
          "ec2:Delete*",
          "vpc:Delete*"
        ],
        "Resource": "*"
      }
    ]
  }